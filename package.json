  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^10.0.1",
    "aws-sdk": "^2.851.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "browserslist": "^4.16.0",
    "caniuse-lite": "^1.0.30001171",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^4.0.3",
    "pug": "^3.0.0",
    "sass-loader": "^10.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:server": "babel src --out-dir dist --ignore 'src/assets' --ignore 'src/static' --ignore 'src/webpack.config.js'",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "copyAll": "xcopy src\\static dist\\static /d /e /h /c /y /i && xcopy src\\views dist\\views /d /e /h /c /y /i",
    "build": "npm run build:server && npm run build:assets && npm run copyAll ",
    "start": "PRODUCTION=true node dist/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "mini-css-extract-plugin": "^0.11.3",
    "nodemon": "^2.0.4"
  }
}
